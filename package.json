{
  "name": "supakoto",
  "type": "module",
  "version": "0.0.1",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "build:ci": "ASTRO_COMPRESS=false ASTRO_LOG_LEVEL=info astro build",
    "build:production": "NODE_ENV=production astro build",
    "preview": "astro preview",
    "astro": "astro",
    "clean": "rimraf node_modules .astro dist",
    "analyze": "ANALYZE=true NODE_ENV=production astro build",
    "analyze:source-map": "npm run build:production && npx source-map-explorer 'dist/assets/js/*.js'",
    "lighthouse": "npx lighthouse http://localhost:4321 --output=html --output-path=./lighthouse-report.html",
    "lighthouse:mobile": "lighthouse http://localhost:4321 --preset=mobile --output=html --output-path=./reports/lighthouse-mobile.html",
    "lighthouse:desktop": "lighthouse http://localhost:4321 --preset=desktop --output=html --output-path=./reports/lighthouse-desktop.html",
    "test:performance": "npm run build:production && npm run preview & sleep 5 && npm run lighthouse:mobile && npm run lighthouse:desktop && kill %1",
    "test:nav": "playwright test scripts/nav-smoke.test.ts"
  },
  "dependencies": {
    "@astrojs/react": "^3.0.0",
    "@astrojs/tailwind": "^5.0.0",
    "@astrojs/vercel": "^8.2.7",
    "@iconify-json/simple-icons": "^1.2.44",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@types/nodemailer": "^7.0.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "astro": "^5.12.9",
    "astro-i18n": "^2.2.4",
    "astro-icon": "^1.1.5",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.541.0",
    "motion": "^12.23.0",
    "nodemailer": "^7.0.6",
    "postcss": "^8.4.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-rtl": "^0.9.0"
  },
  "devDependencies": {
    "@astrojs/sitemap": "^3.5.1",
    "@iconify-json/mdi": "^1.2.3",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.20",
    "astro-compress": "^2.3.8",
    "bundlesize": "^0.18.1",
    "imagemin": "^8.0.1",
    "imagemin-webp": "^8.0.0",
    "lighthouse": "^12.8.1",
    "purgecss": "^5.0.0",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "sharp": "^0.34.2",
    "source-map-explorer": "^2.5.3",
    "ts-prune": "^0.10.3"
  }
}
